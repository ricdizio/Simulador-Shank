{
  "name": "simuladorshank",
  "version": "1.0.0",
  "description": "Simulador de laberintos",
  "main": "index.js",
  "dependencies": {
    "electron": "^1.6.5"
  },
  "devDependencies": {},
  "scripts": {
    "start": "electron ./",
    "build-osx": "electron-packager . ShankSimulador --version 1.6.2 --platform darwin --icon ./assets/logo.icns --prune=true --out ./dist --overwrite",
    "build-linux": "electron-packager . ShankSimulador --version 1.6.2 --platform linux --icon ./assets/logo.png --prune=true --out ./dist --overwrite",
    "build-win": "electron-packager . ShankSimulador --version 1.6.2 --platform win32 --icon ./assets/logo.ico --prune=true --out ./dist --path ./ --overwrite"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ricdizio/Simulador-Shank.git"
  },
  "author": "ricdizio / Gabriel Noya",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ricdizio/Simulador-Shank/issues"
  },
  "homepage": "https://github.com/ricdizio/Simulador-Shank#readme"
}
